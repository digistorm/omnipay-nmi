{
    "name": "digistorm/omnipay-nmi",
    "type": "library",
    "description": "NMI (Network Merchants Inc.) driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "nmi",
        "durango",
        "network",
        "inc"
    ],
    "homepage": "https://github.com/digistorm/omnipay-nmi",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Omnipay\\NMI\\" : "src/" }
    },
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "http-interop/http-factory-guzzle": "^1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
